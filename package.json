{
  "name": "wonder-inzenjering-api",
  "version": "1.0.0",
  "description": "Wonder Inzenjering - Admin Panel API - manage Construction Sites, Employees, Wage Reports & Financial Reports",
  "keywords": [],
  "author": "Nemanja Kuzmanovic",
  "license": "GPL-3.0-or-later",
  "main": "./dist/server.js",
  "private": true,
  "scripts": {
    "start": "node ./dist/server.js",
    "start-pm2": "pm2-runtime start ecosystem.config.js --env production",
    "build": "tsc -p .",
    "start-ts": "cross-env NODE_ENV=production ts-node src/server.ts",
    "dev": "cross-env NODE_ENV=development nodemon ./src/server.ts",
    "test": "cross-env NODE_ENV=testing nodemon ./src/server.ts",
    "seed": "cross-env NODE_ENV=development ts-node ./src/config/seeder.ts --import",
    "reset": "cross-env NODE_ENV=development ts-node ./src/config/seeder.ts --delete",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.0.0",
    "express-rate-limit": "^5.1.3",
    "helmet": "^3.23.3",
    "hpp": "^0.2.3",
    "http-status": "^1.4.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.25",
    "morgan": "^1.10.0",
    "nodemailer": "^6.4.10",
    "pm2": "^5.2.0",
    "slugify": "^1.4.5",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.7",
    "@types/express-mongo-sanitize": "^1.3.2",
    "@types/express-rate-limit": "^5.0.0",
    "@types/helmet": "0.0.47",
    "@types/hpp": "^0.2.1",
    "@types/http-status": "^1.1.2",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.7.31",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.26",
    "@types/nodemailer": "^6.4.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.5.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.4",
    "eslint-plugin-react-hooks": "^4.0.7",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "engines": {
    "node": ">= 14.0.0",
    "npm": ">= 6.14.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/denrion/project-wonder"
  }
}
